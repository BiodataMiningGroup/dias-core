{
    "name": "biigle/core",
    "type": "project",
    "description": "Core of the BIIGLE web application.",
    "keywords": [
        "BIIGLE",
        "core"
    ],
    "license": "proprietary",
    "authors": [
        {
            "name": "Martin Zurowietz",
            "email": "martin@cebitec.uni-bielefeld.de"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-exif": "*",
        "ext-json": "*",
        "ext-pgsql": "*",
        "ext-vips": "*",
        "biigle/ananas": "dev-master",
        "biigle/annotations": "dev-master",
        "biigle/color-sort": "dev-master",
        "biigle/demo": "dev-master",
        "biigle/geo": "dev-master",
        "biigle/label-trees": "dev-master",
        "biigle/laravel-remote-queue": "^2.3",
        "biigle/largo": "dev-master",
        "biigle/laserpoints": "dev-master",
        "biigle/maia": "dev-master",
        "biigle/projects": "dev-master",
        "biigle/reports": "dev-master",
        "biigle/sync": "dev-master",
        "biigle/videos": "dev-master",
        "biigle/volumes": "dev-master",
        "biigle/laravel-file-cache": "^3.0",
        "doctrine/annotations": "1.4.0",
        "doctrine/dbal": "^2.5",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "~6.0",
        "jcupitt/vips": "^1.0",
        "laravel/framework": "^6.0",
        "laravel/tinker": "^1.0",
        "mzur/laravel-openstack-swift": "^1.0",
        "msurguy/honeypot": "^1.0",
        "ramsey/uuid": "^3.5",
        "ramsey/uuid-doctrine": "^1.2"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "johnkary/phpunit-speedtrap": "^3.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Biigle\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Biigle\\Tests\\": "tests/php/"
        },
        "classmap": [
            "tests/TestCase.php",
            "tests/ApiTestCase.php",
            "tests/ModelTestCase.php",
            "tests/CreatesApplication.php",
            "tests/Bootstrap.php"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "test": [
            "docker-compose run --rm worker php -d memory_limit=1G vendor/bin/phpunit --colors=always --random-order"
        ],
        "testf": [
            "docker-compose run --rm worker php -d memory_limit=1G vendor/bin/phpunit --colors=always --filter"
        ],
        "fix": [
            "php-cs-fixer fix --config='.php_cs'"
        ],
        "doc": [
            "rm -rf public/doc/server",
            "php sami.phar update sami.php -v"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        "0": {
            "type": "vcs",
            "url": "git@github.com:biigle/projects.git"
        },
        "1": {
            "type": "vcs",
            "url": "git@github.com:biigle/volumes.git"
        },
        "2": {
            "type": "vcs",
            "url": "git@github.com:biigle/annotations.git"
        },
        "3": {
            "type": "vcs",
            "url": "git@github.com:biigle/color-sort.git"
        },
        "4": {
            "type": "vcs",
            "url": "git@github.com:biigle/label-trees.git"
        },
        "5": {
            "type": "vcs",
            "url": "git@github.com:biigle/largo.git"
        },
        "6": {
            "type": "vcs",
            "url": "git@github.com:biigle/laserpoints.git"
        },
        "7": {
            "type": "vcs",
            "url": "git@github.com:biigle/reports.git"
        },
        "8": {
            "type": "vcs",
            "url": "git@github.com:biigle/geo.git"
        },
        "9": {
            "type": "vcs",
            "url": "git@github.com:biigle/ananas.git"
        },
        "10": {
            "type": "vcs",
            "url": "git@github.com:biigle/sync.git"
        },
        "12": {
            "type": "vcs",
            "url": "git@github.com:biigle/demo.git"
        },
        "13": {
            "type": "vcs",
            "url": "https://github.com/biigle/laravel-image-cache"
        },
        "maia": {
            "type": "vcs",
            "url": "git@github.com:biigle/maia.git"
        },
        "videos": {
            "type": "vcs",
            "url": "git@github.com:biigle/videos.git"
        }
    }
}
