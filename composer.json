{
    "name": "biigle/core",
    "type": "project",
    "description": "Core of the BIIGLE web application.",
    "keywords": ["BIIGLE", "core"],
    "license": "proprietary",
    "authors": [
        {
            "name": "Martin Zurowietz",
            "email": "martin@cebitec.uni-bielefeld.de"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-exif": "*",
        "ext-json": "*",
        "ext-pgsql": "*",
        "ext-vips": "*",
        "biigle/laravel-file-cache": "^2.0",
        "doctrine/annotations": "1.4.0",
        "doctrine/dbal": "^2.5",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "~6.0",
        "jcupitt/vips": "^1.0",
        "laravel/framework": "5.8.*",
        "laravel/tinker": "^1.0",
        "msurguy/honeypot": "^1.0",
        "predis/predis": "^1.1",
        "ramsey/uuid": "^3.5",
        "ramsey/uuid-doctrine": "^1.2"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "johnkary/phpunit-speedtrap": "^3.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Biigle\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Biigle\\Tests\\": "tests/php/"
        },
        "classmap": [
            "tests/TestCase.php",
            "tests/ApiTestCase.php",
            "tests/ModelTestCase.php",
            "tests/CreatesApplication.php"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "test": [
            "docker-compose run --rm worker php -d memory_limit=1G vendor/bin/phpunit --colors=always"
        ],
        "testf": [
            "docker-compose run --rm worker php -d memory_limit=1G vendor/bin/phpunit --colors=always --filter"
        ],
        "fix": [
            "php-cs-fixer fix --config='.php_cs'"
        ],
        "doc": [
            "rm -rf public/doc/server",
            "php sami.phar update sami.php -v"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
