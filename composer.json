{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"type": "project",
	"require": {
		"laravel/framework": "5.1.*",
		"intervention/image": "~2.1",
		"dias/projects": "dev-master",
		"dias/transects": "dev-master",
		"dias/annotations": "dev-master"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.0",
		"sami/sami": "dev-master",
		"mockery/mockery": "dev-master"
	},
	"autoload": {
		"classmap": [
			"database"
		],
		"psr-4": {
			"Dias\\": "app/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php",
			"tests/ApiTestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php -r \"copy('.env.example', '.env');\"",
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "porta.cebitec.uni-bielefeld.de:/vol/biodtmin/git/dias-projects.git"
		},
		{
			"type": "vcs",
			"url": "porta.cebitec.uni-bielefeld.de:/vol/biodtmin/git/dias-transects.git"
		},
		{
			"type": "vcs",
			"url": "porta.cebitec.uni-bielefeld.de:/vol/biodtmin/git/dias-annotations.git"
		}
	]
}
